<?xml version="1.0" encoding="UTF-8"?>
<aiml>
  <category>
      <pattern>HELLO</pattern>
      <template>Hello Student! What is your name?</template>
  </category>
  
  <category>
      <pattern>HELLO *</pattern>
      <template>Hello Student! What is your name?
        </template>
    </category>
  

<category>
    <pattern>MY NAME IS *</pattern>
    <template>Hello <set name="firstname"><star/></set>!<br/>
    Do you enjoy working with people and tech, or with code and datasets?
    <button><text>People and Tech</text>
      <postback>People and Tech</postback>
    </button>
    <button>
      <text>Code and Datasets</text>
      <postback>Code and Datasets</postback>
        </button>
    
    </template>
    
    </category>
    
    
