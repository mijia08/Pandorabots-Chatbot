    <!-- IT Technician, Network Engineer, Cybersecurity -->
<category>
    <pattern>PEOPLE AND TECH</pattern>
    <template>Which strength do you align with?
    <button>
      <text>Hardware Repair</text>
      <postback>Hardware Repair</postback>
    </button>
    <button>
      <text>Network Infrastructure</text>
      <postback>Network Infrastructure</postback>
        </button>
    <button>
        <text>Threat Response</text>
        <postback>Threat Response</postback>
    </button>
        </template>
        </category>

    
    <!-- SWE, DS -->
<category>   
    <pattern>CODE AND DATASETS</pattern>
     <template>Which strength do you align with?
   <button><text>Testing and Debugging</text>
      <postback>Testing and Debugging</postback>
    </button>
    <button>
      <text>Storytelling with Datasets</text>
      <postback>Storytelling with Datasets</postback>
        </button>
     </template>
    </category>
  
 
 <!-- More Definitive Questions -->  
 
<!-- SWE, DS -->
  <category>
    <pattern>TESTING AND DEBUGGING</pattern>
    <template>Do you enjoy building applications from scratch and resolving bugs?
    <button><text>Yes, I enjoy building applications from scratch and resolving bugs</text>
      <postback>Yes, I enjoy building applications from scratch and resolving bugs</postback>
    </button>
    <button>
      <text>No, I do not enjoy building applications from scratch and resolving bugs</text>
      <postback>No, I do not enjoy building applications from scratch and resolving bugs</postback>
        </button>
    </template>
  </category>
 
  <category>
    <pattern>STORYTELLING WITH DATASETS</pattern>
    <template>Do you enjoy making stories from messy and large datasets using Python or R?
